---

import Layout from '../layouts/Layout.astro';
import LandingHeader from '../components/LandingHeader.astro';
import HeroSection from '../components/HeroSection.astro';
import ModelSection from '../components/ModelY.astro';
import ModelY from '../components/ModelY.astro';
import ModelX from '../components/ModelX.astro';
import Contacto from '../components/Contacto.astro';
import FooterMenu from '../components/FooterMenu.astro';

---

<Layout title="MountainBike Ushuaia">
	<LandingHeader/>
	<div class=" mb"></div>
	
	<main class=" snap-y snap-mandatory relative w-full h-screen overflow-auto scroll-smooth">
		<div class=" snap-center">
			
			<HeroSection />	
		<div/>
		
		<div class=" snap-center">
			<ModelY></ModelY> 	
		<div/>
		<div class=" snap-center">
			<ModelX></ModelX> 	
		<div/>

		<div class=" snap-center">
			<Contacto></Contacto> 	
		<div/>
	


	</main>
		
</Layout>
