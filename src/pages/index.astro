import Moove from '../components/Moove.astro';
import Slp from '../components/Slp.astro';
import Firebird from '../components/Firebird.astro';
---

<Layout title="MountainBike Ushuaia">
	<LandingHeader/>
	<div class=" mb"></div>
	
	<main class=" snap-y snap-mandatory relative w-full h-screen overflow-auto scroll-smooth">

		
		<div class=" snap-center">
			<HeroSection />	
		<div/>
		
		<div class=" snap-center">
			<Venzo></Venzo> 	
		<div/>
		<div class=" snap-center">
			<Zion></Zion> 	
		<div/>
		<div class=" snap-center">
			<Moove></Moove> 	
		<div/>
		<div class=" snap-center">
			 <Slp></Slp>	
		<div/>
		<div class=" snap-center">
			 <Firebird></Firebird>	
		<div/>

		
